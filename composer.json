{
  "name": "luyunhua/mframe",
  "description": "The Micro Mobile PHP Framework",
  "keywords": ["framework", "luyunhua"],
  "license": "MIT",
  "type": "project",
  "require": {
    "php": ">=5.5.9",
    "luyunhua/croute": "dev-master",
    "luyunhua/framework": "dev-master"
  },
  "autoload":{
    "psr-4": {
      "App\\": "app/"
    }
  },
  "repositories": {
    "packagist": {
      "type": "composer",
      "url": "https://packagist.phpcomposer.com"
    },
    "croute": {
      "type": "vcs",
      "url": "git@github.com:luyunhua/croute.git"
    },
    "framework": {
      "type": "vcs",
      "url": "git@github.com:luyunhua/framework.git"
    }
  }
}
